{
  "files.associations": {
    "**/*.html": "html",
    "**/templates/*/*.html": "django-html",
    "**/templates/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements"
  },

  "emmet.includeLanguages": {
    "django-html": "html"
  },

  "[django-html]": {
    "editor.defaultFormatter": "batisteo.vscode-django"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "askForPassword": true,
      "driver": "PostgreSQL",
      "name": "restaurant_db",
      "database": "Food_Restaurant",
      "username": "postgres"
    }
  ]
}
